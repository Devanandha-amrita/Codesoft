    {
      "cell_type": "code",
      "source": [
        "def generate_text(inp):\n",
        "  input_ids=tokenizer.encode(inp,return_tensors=\"tf\")\n",
        "  beam_op=model.generate(input_ids,max_length=100,num_beams=5,no_repeat_ngram_size=2,early_stopping=True)\n",
        "  op=tokenizer.decode(beam_op[0],skip_special_tokens=True,clean_up_tokenization_spaces=True)\n",
        "  return \".\".join(op.split(\".\")[:-1])+\".\""
      ],
      "metadata": {
        "id": "gV5r7FWoMOeK"
      },
      "execution_count": 4,
      "outputs": []
    },
